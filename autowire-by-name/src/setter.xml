<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />
<bean id="emp" class="client_details.client">
<property name="id" value="201"/>
<property name="name" value="wisconson"/>
</bean>
<bean id="address" class="client_address.Address">
<property name="streeth_address" value="420 atlantic streeth "/>
<property name="city" value="bridgeport"/>
<property name="state" value="connecticut"/>
</bean>
</beans>